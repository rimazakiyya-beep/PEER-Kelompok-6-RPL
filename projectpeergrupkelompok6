#include <iostream>
using namespace std;

// Fungsi untuk menentukan kategori usia
string kategoriUsia(int umur) {
    if (umur >= 0 && umur <= 12) {
        return "Anak-anak";
    } else if (umur >= 13 && umur <= 17) {
        return "Remaja";
    } else if (umur >= 18 && umur <= 59) {
        return "Dewasa";
    } else if (umur >= 60) {
        return "Lansia";
    } else {
        return "Umur tidak valid!";
    }
}
int main() {
    int jumlahOrang;
    cout << "Masukkan jumlah orang yang ingin dicek: ";
    cin >> jumlahOrang;
for (int i = 1; i <= jumlahOrang; i++) {
    int umur;
    
}
